{
    "name": "drodata/yii2-app-template",
    "description": "Improved Version of Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template", "drodata", "yat"],
    "authors": [
        {
            "name": "drodata",
            "email": "drodata@foxmail.com"
        }
    ],
    "homepage": "https://github.com/drodata/yii2-app-template",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/drodata/yii2-app-template/issues",
        "source": "https://github.com/drodata/yii2-app-template"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.13",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-swiftmailer": "2.0.6",
        "yiisoft/yii2-imagine": "2.0.4",
        "almasaeed2010/adminlte": "2.3.7",
        "alexgx/yii2-phpexcel": "@dev",
        "bower-asset/chartjs": "2.3.0",
        "bower-asset/weui": "1.1.1",
        "kartik-v/yii2-widget-select2": "2.0.8",
        "kartik-v/yii2-date-range": "1.6.8",
        "kartik-v/yii2-mpdf": "~1.0",
        "picqer/php-barcode-generator": "0.2.1",
        "drodata/yii2-utility": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "yiisoft/yii2-faker": "~2.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
