{
    "name": "drodata/yii2-app-template",
    "description": "Improved Version of Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template", "drodata", "yat"],
    "authors": [
        {
            "name": "drodata",
            "email": "drodata@foxmail.com"
        }
    ],
    "homepage": "https://github.com/drodata/yii2-app-template",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/drodata/yii2-app-template/issues",
        "source": "https://github.com/drodata/yii2-app-template"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "dev-master",
        "yiisoft/yii2-swiftmailer": "*",
        "drodata/yii2-utility": "@dev",
        "almasaeed2010/adminlte": "~2.0",
        "alexgx/yii2-phpexcel": "@dev",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "picqer/php-barcode-generator": "^0.2.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
